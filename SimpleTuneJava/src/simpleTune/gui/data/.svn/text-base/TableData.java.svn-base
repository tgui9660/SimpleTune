package simpleTune.gui.data;

import java.io.Serializable;

public class TableData implements Serializable{

	public static int CVS = 0;
	public static int ROM = 0;
	
	private Double[][] tableData;
	private String name;
	private int colCount;
	private int rowCount;
	private String[] colHeaders;
	private String[] rowHeaders;
	
	
	public TableData(String name){
		this.name = name;
	}
	
	public int getColCount() {
		return colCount;
	}
	public void setColCount(int colCount) {
		this.colCount = colCount;
	}
	public String[] getColHeaders() {
		return colHeaders;
	}
	public void setColHeaders(String[] colHeaders) {
		this.colHeaders = colHeaders;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getRowCount() {
		return rowCount;
	}
	public void setRowCount(int rowCount) {
		this.rowCount = rowCount;
	}
	public String[] getRowHeaders() {
		return rowHeaders;
	}
	public void setRowHeaders(String[] rowHeaders) {
		this.rowHeaders = rowHeaders;
	}
	public Double[][] getTableData() {
		return tableData;
	}
	public void setTableData(Double[][] tableData) {
		this.tableData = tableData;
	}
}
